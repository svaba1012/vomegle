<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.vomegleclient.NewChatController"
      prefHeight="400.0" prefWidth="600.0"
      alignment="CENTER"
      stylesheets="@styles.css"
        spacing="30">

    <Button styleClass="acceptBtn,bigBtn" fx:id="newChatButton" onAction="#onNewChatButtonClick" defaultButton="true" text="Нови чет"></Button>

    <Button styleClass="cancelBtn,bigBtn" fx:id="exitAppButton" onAction="#onExitAppButtonClick" text="Излаз" cancelButton="true"></Button>

</HBox>
